{
  "include": ["./src"],
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "moduleResolution": "Node",
    "jsx": "react-jsx",
    "strict": true,
    "baseUrl": "src",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "paths": {
      "@components*": ["components*"],
      "@pages*": ["pages*"],
      "@app*": ["app*"],
    }
  }
}